{
  "hosting": [
    {
      "public": "dist",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "www",
      "redirects": [
        {
          "source": "/**",
          "destination": "https://swaminisecurity.in",
          "type": 301
        }
      ]
    }
  ]
}
